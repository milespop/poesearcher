{
  "name": "poesearcher",
  "version": "1.0.0",
  "description": "A Chrome extension that automates item searching on the Path of Exile trade website by parsing item text and automatically filling in search filters.",
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.2.0",
    "@types/chrome": "^0.1.12",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.6"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "build:force": "vite build --force",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run typecheck",
    "postinstall": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milespop/poesearcher.git"
  },
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/milespop/poesearcher/issues"
  },
  "homepage": "https://github.com/milespop/poesearcher#readme"
}
